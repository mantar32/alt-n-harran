# AltınSarraf - Environment Variables
# Kopyalayın ve .env olarak kaydedin

# API Anahtarları (isteğe bağlı - ücretsiz API'ler kullanılıyor)
# EXCHANGE_API_KEY=your_api_key_here
# METALS_API_KEY=your_metals_api_key_here

# Admin Kimlik Bilgileri (üretimde değiştirin!)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# Uygulama Ayarları
DEFAULT_REFRESH_INTERVAL=15
DEFAULT_BUY_SPREAD=0.3
DEFAULT_SELL_SPREAD=0.3

# Önbellek Süresi (saniye)
CACHE_DURATION=10
